        <!--<TextBox x:Name="TextBoxDataLogu" HorizontalAlignment="Left" Margin="107,850,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>-->


        <Button x:Name="ButtonWyszukaj" Content="Wyszukaj" HorizontalAlignment="Left" Margin="1208,595,0,0" VerticalAlignment="Top" Click="ButtonWyszukaj_Click" Grid.Row="1" Height="38" Width="135"/>
        <TextBox x:Name="TextBoxCzyZatwierdzony" HorizontalAlignment="Left" Margin="1369,360,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Row="1"/>
        <DatePicker x:Name="DatePickerData" HorizontalAlignment="Left" Margin="1369,409,0,0" VerticalAlignment="Top" Grid.Row="1" Width="120"/>
        <TextBox x:Name="TextBoxTyp" HorizontalAlignment="Left" Margin="1369,460,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Row="1"/>
