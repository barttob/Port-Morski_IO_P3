        <TextBlock HorizontalAlignment="Left" Margin="1044,366,0,0" TextWrapping="Wrap" Text="Numer" VerticalAlignment="Top" Grid.Row="1"/>
        <TextBlock HorizontalAlignment="Left" Margin="1044,416,0,0" TextWrapping="Wrap" Text="Operacja" VerticalAlignment="Top" Grid.Row="1"/>
