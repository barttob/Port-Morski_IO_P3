        <TextBlock HorizontalAlignment="Left" Margin="1290,413,0,0" Grid.Row="1" TextWrapping="Wrap" Text="Data" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="1290,464,0,0" Grid.Row="1" TextWrapping="Wrap" Text="Typ" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="1291,513,0,0" Grid.Row="1" TextWrapping="Wrap" Text="Data logu" VerticalAlignment="Top"/>
        <TextBox x:Name="TextBoxNumer" HorizontalAlignment="Left" Margin="1122,362,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Row="1"/>
        <TextBox x:Name="TextBoxOperacja" HorizontalAlignment="Left" Margin="1122,412,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Grid.Row="1"/>
